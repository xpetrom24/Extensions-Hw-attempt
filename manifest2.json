{
  "manifest_version": 2,
  "name": "Juxtapoz",
  "description": "This extension adds clickable buttons to change background to fine art"
  "version": "0.0.1",
"content_scripts": [
    {
      "matches": ["http://*.juxtapoz.com/*"],
      "css": ["stylejuxtapoz.css"],
      "js": ["jquery.js", "script.js"]      
    }
  ]
 
  
}
